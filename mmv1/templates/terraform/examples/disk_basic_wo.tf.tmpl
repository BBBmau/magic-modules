resource "google_compute_disk" "default" {
  name  = "{{index $.Vars "disk_name"}}"
  type  = "pd-ssd"
  zone  = "us-central1-a"
  image = "debian-11-bullseye-v20220719"
  labels = {
    environment = "dev"
  }
  disk_encryption_key_wo = {
    raw_key_wo = "raw_key_wo"
    rsa_encrypted_key_wo = "rsa_encrypted_key_wo"
  }
  physical_block_size_bytes = 4096
}
