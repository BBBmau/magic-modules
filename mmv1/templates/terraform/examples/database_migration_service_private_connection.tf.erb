resource "google_database_migration_service_private_connection" "<%= ctx[:primary_resource_id] %>" {
	display_name          = "dbms_pc"
	location              = "us-central1"
	private_connection_id = "<%= ctx[:vars]['private_connection_id'] %>"

	labels = {
		key = "value"
	}

	vpc_peering_config {
		vpc_name = data.google_compute_network.private_connection.id
		subnet = "10.128.0.0/9"
	}

	depends_on = [google_compute_network.private_connection]
}

resource "google_compute_network" "private_connection" {
  name = "<%= ctx[:vars]['network_name'] %>"
  auto_create_subnetworks = true
}
